<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Happy.happythrow">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/notitle">
        <activity
            android:name="com.Happy.happythrow.my_trash"
            android:exported="false" />
        <activity
            android:name="com.Happy.happythrow.setting"
            android:exported="false" />
        <activity
            android:name="com.Happy.happythrow.QR_main"
            android:exported="false" />
        <activity
            android:name="com.Happy.happythrow.trash_chart"
            android:exported="false" />
        <activity
            android:name="com.Happy.happythrow.adduser"
            android:windowSoftInputMode="adjustResize"
            android:exported="false" />
        <activity android:name="com.Happy.happythrow.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>